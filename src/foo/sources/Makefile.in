# Makefile generated by OpenModelica
CC=@CC@
AR=@AR@
CFLAGS=@CFLAGS@
LD=$(CC) -shared
LDFLAGS=@LDFLAGS@ @LIBS@
DLLEXT=@DLLEXT@
NEED_RUNTIME=@NEED_RUNTIME@
NEED_DGESV=@NEED_DGESV@
FMIPLATFORM=@FMIPLATFORM@
# Note: Simulation of the fmu with dymola does not work with -finline-small-functions (enabled by most optimization levels)
CPPFLAGS=@CPPFLAGS@
OMC_NUM_LINEAR_SYSTEMS=0
OMC_NUM_NONLINEAR_SYSTEMS=0
OMC_NUM_MIXED_SYSTEMS=0
OMC_NDELAY_EXPRESSIONS=0
OMC_NVAR_STRING=0

override CPPFLAGS += -Iinclude/ -Iinclude/fmi2 -I.    -DOMC_MODEL_PREFIX=FmuExportCrossCompile -DOMC_NUM_MIXED_SYSTEMS=0 -DOMC_NUM_LINEAR_SYSTEMS=0 -DOMC_NUM_NONLINEAR_SYSTEMS=0 -DOMC_NDELAY_EXPRESSIONS=0 -DOMC_NVAR_STRING=0

MAINFILE=FmuExportCrossCompile_FMU.c
MAINOBJ=FmuExportCrossCompile_FMU.o
CFILES=FmuExportCrossCompile.c FmuExportCrossCompile_functions.c FmuExportCrossCompile_records.c \
FmuExportCrossCompile_01exo.c FmuExportCrossCompile_02nls.c FmuExportCrossCompile_03lsy.c FmuExportCrossCompile_04set.c FmuExportCrossCompile_05evt.c FmuExportCrossCompile_06inz.c FmuExportCrossCompile_07dly.c \
FmuExportCrossCompile_08bnd.c FmuExportCrossCompile_09alg.c FmuExportCrossCompile_10asr.c FmuExportCrossCompile_11mix.c FmuExportCrossCompile_12jac.c FmuExportCrossCompile_13opt.c FmuExportCrossCompile_14lnz.c \
FmuExportCrossCompile_15syn.c FmuExportCrossCompile_16dae.c FmuExportCrossCompile_init_fmu.c
OFILES=$(CFILES:.c=.o)
GENERATEDFILES=$(MAINFILE) FmuExportCrossCompile_FMU.makefile FmuExportCrossCompile_literals.h FmuExportCrossCompile_model.h FmuExportCrossCompile_includes.h FmuExportCrossCompile_functions.h  FmuExportCrossCompile_11mix.h FmuExportCrossCompile_12jac.h FmuExportCrossCompile_13opt.h FmuExportCrossCompile_init_fmu.c FmuExportCrossCompile_info.c $(CFILES) FmuExportCrossCompile_FMU.libs

# FIXME: before you push into master...
RUNTIMEDIR=include
OMC_MINIMAL_RUNTIME=1
OMC_FMI_RUNTIME=1
include $(RUNTIMEDIR)/Makefile.objs
ifneq ($(NEED_RUNTIME),)
RUNTIMEFILES=$(FMI_ME_OBJS:%=$(RUNTIMEDIR)/%.o)
endif

PHONY: FmuExportCrossCompile_FMU
FmuExportCrossCompile_FMU: $(MAINOBJ) FmuExportCrossCompile_functions.h FmuExportCrossCompile_literals.h $(OFILES) $(RUNTIMEFILES)
	mkdir -p ../binaries/$(FMIPLATFORM)
ifeq (@LIBTYPE_DYNAMIC@,1)
	$(LD) -o FmuExportCrossCompile$(DLLEXT) $(MAINOBJ) $(OFILES) $(RUNTIMEFILES)    $(LDFLAGS)
	cp FmuExportCrossCompile$(DLLEXT) FmuExportCrossCompile_FMU.libs config.log ../binaries/$(FMIPLATFORM)/
endif
ifeq (@LIBTYPE_STATIC@,1)
	rm -f FmuExportCrossCompile.a
	$(AR) -rsu FmuExportCrossCompile.a $(MAINOBJ) $(OFILES) $(RUNTIMEFILES)
	cp FmuExportCrossCompile.a FmuExportCrossCompile_FMU.libs config.log ../binaries/$(FMIPLATFORM)/
endif
	$(MAKE) distclean
	cd .. && rm -f ../FmuExportCrossCompile.fmu && zip -r ../FmuExportCrossCompile.fmu *
distclean: clean
	rm -f Makefile config.status config.log
clean:
	rm -f FmuExportCrossCompile.def FmuExportCrossCompile.o FmuExportCrossCompile.a FmuExportCrossCompile$(DLLEXT) $(MAINOBJ) $(OFILES) $(RUNTIMEFILES)